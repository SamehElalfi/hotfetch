{
  "name": "hotfetch",
  "version": "1.0.0",
  "description": "Scrape pages and extract data in ease",
  "main": "src/index.ts",
  "scripts": {
    "test": "jest --coverage",
    "start": "npx ts-node ./src ./dist",
    "build": "tsc ./src/index.ts --outDir ./dist",
    "build:watch": "tsc -w ./src/index.ts --outDir ./dist",
    "dev": "nodemon ./dist/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samehelalfi/hotfetch.git"
  },
  "keywords": [
    "scrape",
    "fetch",
    "crawler",
    "parser",
    "web",
    "dom"
  ],
  "author": "Sameh Elalfi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samehelalfi/hotfetch/issues"
  },
  "homepage": "https://github.com/samehelalfi/hotfetch#readme",
  "devDependencies": {
    "@babel/core": "^7.17.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@jest/globals": "^27.5.1",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "babel-jest": "^27.5.1",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^26.1.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "typescript": "^4.6.2"
  }
}